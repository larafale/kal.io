doctype html

html
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no, minimal-ui')
    title #{title}
    link(rel='stylesheet', href='/css/bootstrap.3.2.0.css')
    link(rel='stylesheet', type='text/css', href='/css/kal.io.css')
    link(rel='stylesheet', type='text/css', href='/css/utils.css')
    link(rel='stylesheet', type='text/css', href='/css/fa.css')
    link(rel='stylesheet', type='text/css', href='/css/toastr.css')
    script(src='/js/jquery.js')
    script(src='/js/bootstrap.3.2.0.js')
    script(src='/js/lodash.js')
    script(src='/js/vue.js')
    script(src='/js/moment.js')
    script(src='/js/apy.js')
    script(src='/js/api.js')
    script(src='/js/toastr.min.js')
    script(src='/js/fastclick.js')
    script(src='/js/init.js')

    
    style.
      [v-cloak] { display: none }
      html, body {
        height: 100%;
      }

    
  block js

        
  body(v-cloak)
    block body
    block script
